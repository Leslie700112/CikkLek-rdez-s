                         LTRIM(RTRIM(Cikk.SC01039)) AS AccCodeStrg, RIGHT(Sorok.OR21002, 3) AS LineNumbers, Reszl.OR23034 AS InvoiceFull, Vev.SL01002 AS CustName
FROM            OR200300 WITH (nolock) INNER JOIN
                         OR230300 AS Reszl WITH (nolock) ON OR200300.OR20001 = Reszl.OR23001 AND OR200300.OR20021 = Reszl.OR23034 LEFT OUTER JOIN
                         OR210300 AS Sorok WITH (nolock) ON Reszl.OR23001 = Sorok.OR21001 AND Reszl.OR23002 = Sorok.OR21002 AND Reszl.OR23034 = Sorok.OR21065 INNER JOIN
                         SC010300 AS Cikk WITH (nolock) ON Sorok.OR21005 = Cikk.SC01001 LEFT OUTER JOIN
                         SL010300 AS Vev WITH (nolock) ON OR200300.OR20003 = Vev.SL01001
WHERE        (Sorok.OR21005 = @cikk)</CommandText>
