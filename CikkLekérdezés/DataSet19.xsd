                         Sorok.OR21067 AS PurcaseOrNo, Reszl.OR23006 AS QtyRequested, Reszl.OR23007 AS QtyDelivered, Reszl.OR23009 AS DelivDateReq, Reszl.OR23011 AS DelivDateAct, RIGHT(Reszl.OR23034, 6) AS InvoiceNo,
