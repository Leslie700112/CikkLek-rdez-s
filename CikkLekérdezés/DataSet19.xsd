                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="false">
                    <CommandText>SELECT        OR200300.OR20001 AS OrderNo, OR200300.OR20002 AS OrderType, OR200300.OR20003 AS CustCodeInv, OR200300.OR20072 AS CustPONo, OR200300.OR20056 AS ShipmentMark, 
