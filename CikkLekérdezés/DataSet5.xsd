                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="false">
                    <CommandText>SELECT        VF_ST080300_PurchaseStat.SupplierCode, VF_ST080300_PurchaseStat.OrderNo, VF_ST080300_PurchaseStat.InvoiceDate1, 
                         VF_ST080300_PurchaseStat.DeliveryDate, VF_ST080300_PurchaseStat.StockCode, VF_ST080300_PurchaseStat.ProductGroup, 
                         VF_ST080300_PurchaseStat.PriceLCU, VF_ST080300_PurchaseStat.Price, VF_ST080300_PurchaseStat.QtySC, VF_ST080300_PurchaseStat.CurrencyCode, 
                         VF_ST080300_PurchaseStat.InvoiceNo, VF_PL010300_SupplierFile.SupplierName, VF_SY240300_termcsop.TextDescription, 
                         VF_PL030300_PurchaseLedger.InternRefNo, PC43.DelivNoteNo
FROM            VF_ST080300_PurchaseStat LEFT OUTER JOIN
                         VF_PL030300_PurchaseLedger ON VF_ST080300_PurchaseStat.InvoiceNo = VF_PL030300_PurchaseLedger.InvoiceNo LEFT OUTER JOIN
                         VF_SY240300_termcsop ON VF_ST080300_PurchaseStat.ProductGroup = VF_SY240300_termcsop.Key1 LEFT OUTER JOIN
                         VF_PL010300_SupplierFile ON VF_ST080300_PurchaseStat.SupplierCode = VF_PL010300_SupplierFile.SupplierCode LEFT OUTER JOIN
                         VF_PC430300 AS PC43 ON VF_ST080300_PurchaseStat.InvoiceNo = PC43.InvoiceNo_Last AND 
                         VF_ST080300_PurchaseStat.BatchIDDeliv = PC43.BatchNumber
WHERE        (VF_ST080300_PurchaseStat.StockCode = @v_cikk)
ORDER BY VF_ST080300_PurchaseStat.DeliveryDate DESC</CommandText>
