                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="false">
                    <CommandText>SELECT        StockCode, SUM(StockBalance) AS Expr1, SUM(ReservedQty) AS Expr2, SUM(BackOrdQty) AS Expr3, SUM(QtyOrdered1) AS Expr4, SUM(LockedQty) AS Expr5, 
                         SUM(AllocQty) AS Expr6, CONVERT(decimal(15, 4),
                             (SELECT        CASE WHEN MAX(SC01.SC01234) = 0 THEN CASE WHEN SUM(SC03003) &gt; 0 THEN SUM(SC03057 * SC03003) / SUM(SC03003) 
                                                         ELSE CASE WHEN MAX(SC01.SC01052) &gt; 0 THEN MAX(SC01.SC01052) ELSE MAX(SC01.SC01053) END END ELSE MAX(SC01.SC01053) 
                                                         END AS Expr1
                               FROM            SC030300 AS SC3 LEFT OUTER JOIN
                                                         SC010300 AS SC01 ON SC3.SC03001 = SC01.SC01001
                               WHERE        (SC03.StockCode = SC3.SC03001))) AS SulyAtlAr
FROM            VF_SC030300_StockWarehouse AS SC03
WHERE        (StockCode = @v_cikk) AND (StockBalance &lt;&gt; 0) OR
                         (StockCode = @v_cikk) AND (ReservedQty &lt;&gt; 0) OR
                         (StockCode = @v_cikk) AND (BackOrdQty &lt;&gt; 0) OR
                         (StockCode = @v_cikk) AND (QtyOrdered1 &lt;&gt; 0)
GROUP BY StockCode</CommandText>
